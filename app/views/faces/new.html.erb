<%= provide(:title, 'Add tabs') %>
<h4>Add tabs</h4>

<div class="row">
  <div class="span12">

    <%= form_for @tab do |f| %>
      <%= render 'shared/error_messages', :object => f.object %>

      <%= f.text_field :name %>

      <%= hidden_field_tag :dashboard_id, @dashboard.id %>
 
      <%= f.submit "Submit", :class => "btn"  %>
    <% end %>
  </div>
</div>