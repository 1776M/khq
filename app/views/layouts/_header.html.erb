<header>
<header class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <%= link_to "kheraHQ", root_path, id: "logo" %>
      <nav>
        <ul class="nav pull-right">
          <% if signed_in? %>
            <% if current_user.name=='mandeep3'%>
              <li><%= link_to "Environment", environment_path %></li> 
              <li><%= link_to "Projects", user_path(current_user) %></li>
              <li><%= link_to "Dashboard", board_path %></li> 
              <li><%= link_to "Data", basecase_path(1) %></li> 
              <li><%= link_to "Groups", groups_path %></li>
            <% elsif current_user.admin? %>
              <li><%= link_to "Projects", user_path(current_user) %></li>
              <li><%= link_to "Dashboard", board_path %></li>
              <li><%= link_to "Data", basecase_path(current_user.group.basecases.last) %></li>
              <li><%= link_to "Group", group_path(current_user.group) %></li>
            <% else %>
              <li><%= link_to "Projects", user_path(current_user) %></li>   
            <% end %>
            <li id="fat-menu" class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                Account <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to "Profile", current_user %></li>
                <li><%= link_to "Settings", edit_user_path(current_user) %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to "Sign out", signout_path, method: "delete" %>
                </li>
              </ul>
            </li>
          <% else %>
            <li><%= link_to "Sign in", signin_path %></li>
          <% end %>
        </ul>
      </nav>
    </div>
  </div>
</header>