<%= provide(:title, 'Add sub tabs') %>
<h4>Add sub tabs</h4>

<div class="row">
  <div class="span6 offset3">

    <%= form_for @sense do |f| %>
      <%= render 'shared/error_messages', :object => f.object %>

      <%= f.text_field :name %>

      <%= hidden_field_tag :face_id, @face.id %>
 
      <%= f.submit "Submit", :class => "btn"  %>
    <% end %>
  </div>
</div>