    <%= f.label :year_two %><br />
    <%= f.text_field :year_2 %>

    <%= f.label :year_three %><br />
    <%= f.text_field :year_3 %>

    <%= f.label :year_four %><br />
    <%= f.text_field :year_4 %>

    <%= f.label :year_five %><br />
    <%= f.text_field :year_5 %>

    <%= f.label :year_six %><br />
    <%= f.text_field :year_6 %>

    <%= f.label :year_seven %><br />
    <%= f.text_field :year_7 %>

    <%= f.label :year_eight %><br />
    <%= f.text_field :year_8 %>

    <%= f.label :year_nine %><br />
    <%= f.text_field :year_9 %>

    <%= f.label :year_ten %><br />
