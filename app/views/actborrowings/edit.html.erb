<%= provide(:title, 'Edit borrowing data') %>
<h1>Edit borrowing data</h1>

<div class="row">
  <div class="span12">

    <%= form_for @actborrowing do |f| %>
      <%= render 'shared/error_messages', :object => f.object %>

      <%= f.label :size %>
      <%= f.text_field :size %>

      <%= f.label :coupon %>
      <%= f.text_field :coupon %>

      <%= f.label :issue_year %>
      <%= f.text_field :issue_year %>

      <%= f.label :maturity_year %>
      <%= f.text_field :maturity_year %>

      <%= f.label :currency %>
      <%= f.text_field :currency %>

      <%= f.label :fixed_float %>
      <%= f.text_field :fixed_float %>
 
      <%= f.submit "Submit", :class => "btn"  %>
    <% end %>
  </div>
</div>